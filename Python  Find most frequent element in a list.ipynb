   "source": [
    "# Python | Find most frequent element in a list"
   ]
  },
  {
   "cell_type": "raw",
   "id": "6fee257a",
   "metadata": {},
   "source": [
    "Input : [2, 1, 2, 2, 1, 3]\n",
    "Output : 2\n",
    "\n",
    "Input : ['Dog', 'Cat', 'Dog']\n",
    "Output : Dog"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "id": "284fa7d8",
   "metadata": {},
   "outputs": [],
   "source": [
    "Input = [2, 1, 2, 2, 1, 3]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "id": "81331290",
   "metadata": {},
   "outputs": [],
   "source": [
    "def frequentElement(Input):\n",
    "    c=[]\n",
    "    for i in Input:\n",
    "        c.append(Input.count(i))\n",
    "    \n",
    "    return c"
   ]
  },
